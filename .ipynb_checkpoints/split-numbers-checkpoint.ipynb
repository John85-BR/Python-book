    "def split_numbers(num):\n",
    "    \n",
    "    temp = 10**len(str(num))\n",
    "    num_list = []\n",
    "\n",
    "    for i in range(len(str(num))):\n",
    "        num_list+=str(((num%temp)//(temp//10)))\n",
    "        temp = temp//10\n",
    "  \n",
    "    return num_list\n",
    "    \n"
   ]
