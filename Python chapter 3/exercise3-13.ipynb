   "source": [
    "num = 50\n",
    "fact = 1\n",
    "\n",
    "def factorial(num):\n",
    "    if num==0:\n",
    "        return 1\n",
    "    \n",
    "    return num * factorial(num-1)\n",
    "\n",
    "for i in range(1,num+1):\n",
    "    fact*=i\n",
    "\n",
    "\n",
    "print(factorial(5)) \n"
   ]
