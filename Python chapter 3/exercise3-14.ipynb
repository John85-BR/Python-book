    "numerador = 4\n",
    "denominador = 3\n",
    "pi = 4\n",
    "i=1\n",
    "\n",
    "while True:\n",
    "    \n",
    "    if i%2==0:\n",
    "        pi+= (numerador/denominador)\n",
    "    else:\n",
    "        pi-= (numerador/denominador)\n",
    "    \n",
    "    denominador+=2\n",
    "    i+=1\n",
    "    \n",
    "    if pi>=3.15 and pi<=3.16 :\n",
    "        print(pi)\n",
    "        print(i)\n",
    "        break\n",
    "    \n"
