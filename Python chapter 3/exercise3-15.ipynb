   "source": [
    "e = 1\n",
    "\n",
    "def factorial(num):\n",
    "    if num==0:\n",
    "        return 1\n",
    "    \n",
    "    return num * factorial(num-1)\n",
    "\n",
    "for i in range(1,11):\n",
    "    \n",
    "    e+=(1/factorial(i))\n",
    "   \n",
    "\n",
    "print(e)"
   ]
