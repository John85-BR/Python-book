    "def seconds_since_midnight(hour, minute, second):\n",
    "   \n",
    "    hour_in_seconds = hour * 60 * 60\n",
    "    minute_in_seconds = minute * 60\n",
    "    return (hour_in_seconds+minute_in_seconds+second)\n",
    "\n",
    "print(seconds_since_midnight(13,30,45))"
   ]
